import java.util.HashMap;

public class ReadingLevelByLengthChart implements Charts{
	
HashMap<Integer, Double[]> dataPairs = new HashMap<>();

public ReadingLevelByLengthChart() {
	this.dataPairs = extractDataPairs();
	
}



public HashMap<Integer, Double[]> extractDataPairs(){
	int articleCount = 1;
	
	for (Article article : Charts.articles) {
		Double length = (double) article.getCharacterCount();
		Double readingLevel = (double) article.getReadingLevel();
		Double[] xy = {length, readingLevel};
		dataPairs.put(articleCount, xy);
		articleCount++;	
		
	}
	
	
	return dataPairs;
}


	
	
}
